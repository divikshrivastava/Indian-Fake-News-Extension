{
    "name": "Indian Fake News Detector",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "home.html",
        "default_icon": "home.png"
    },
    "icons": {
        "128": "home.png"
    },
    "permissions": ["activeTab"],
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["content.js"],
        "matches": ["https://twitter.com/*"]
      }]
}